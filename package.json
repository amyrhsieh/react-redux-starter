{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "A boilerplate React project that comes with webpack, SCSS, and others.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --config webpack.dev.js",
    "build": "rimraf dist/* && webpack --mode production --config webpack.prod.js",
    "deploy": "sh prod_deploy.sh",
    "test": "jest"
  },
  "keywords": [],
  "author": "Amy Hsieh",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "enzyme-to-json": "^3.3.5",
    "final-form": "^4.16.1",
    "fstream": "^1.0.12",
    "react-final-form": "^6.3.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "tar": "^4.4.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/src/__mocks__/styleMock.js",
      "^Src(.*)$": "<rootDir>/src$1",
      "^Mocks(.*)$": "<rootDir>/src/__mocks__$1",
      "^Tests(.*)$": "<rootDir>/src/__tests__$1",
      "^Components(.*)$": "<rootDir>/src/components$1",
      "^Redux(.*)$": "<rootDir>/src/redux$1",
      "^Styles(.*)$": "<rootDir>/src/styles$1",
      "^Services(.*)$": "<rootDir>/src/services$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/enzymeSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
